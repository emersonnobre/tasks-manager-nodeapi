{
  "name": "task-manager-node",
  "version": "1.0.0",
  "description": "a web app for task management",
  "main": "app.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --require dotenv/config src/index.js dotenv_config_path=./environments/dev.env",
    "test": "env-cmd -f ./environments/dev.env jest --detect-open-handles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emersonnobre/tasks-manager.git"
  },
  "keywords": [
    "nodejs",
    "js",
    "task-system"
  ],
  "author": "Emerson Nobre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emersonnobre/tasks-manager/issues"
  },
  "homepage": "https://github.com/emersonnobre/tasks-manager#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "axios": "1.3.4",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "http": "0.0.1-security",
    "jsonwebtoken": "9.0.0",
    "mongoose": "7.0.0",
    "multer": "1.4.5-lts.1",
    "validator": "13.9.0"
  },
  "devDependencies": {
    "env-cmd": "10.1.0",
    "nodemon": "2.0.13",
    "jest": "29.4.3",
    "supertest": "6.3.3"
  }
}
